#include<iostream.h>
#include<conio.h>
class student
{
 int roll;
 static count;
 public:
 student():roll(0)
 {}
 void input()
 {
  cout<<"\nEnter the roll number:";
  cin>>roll;
  countstudent();
 }
 void output()
 {
  cout<<"\nroll number="<<roll;
 }
 void countstudent()
 {
 count++;
 }
}; 
int main()
{
student s;
s.input();
s.output();
getch();
return 0;
}
